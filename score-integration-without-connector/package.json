    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^6.0.0",
    "@types/uuid": "^8.3.4",
    "prettier": "^2.6.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "delay": "^5.0.0",
    "express": "^4.17.3",
    "express-handlebars": "^6.0.3",
    "http-status-codes": "^2.2.0",
    "node-config-ts": "^3.1.0",
    "uuid": "^8.3.2"
